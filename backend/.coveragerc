[run]
source = app
omit = 
    */tests/*
    */test_*
    */__pycache__/*
    */migrations/*
    */alembic/*
    */venv/*
    */env/*
    */node_modules/*
    */build/*
    */dist/*
    */.*
    */conftest.py
    */pytest.ini
    */setup.py
    */requirements*.txt

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

show_missing = True
skip_covered = False
precision = 2

[html]
directory = htmlcov
title = RunMaster AI Features Coverage Report

[xml]
output = coverage.xml
